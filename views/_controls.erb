<div class="controls">
  <form action="/" method="post">
    <button name="command" value="pause"><i class="fa fa-pause"></i></button>
    <button name="command" value="stop"><i class="fa fa-stop"></i></button>
    <button name="command" value="volume -1"><i class="fa fa-volume-down"></i></button>
    <button name="command" value="volume +1"><i class="fa fa-volume-up"></i></button>
    <button name="command" value="seek -10 [0]">-10s</button>
    <button name="command" value="seek 30 [0]">+30s</button>
  </form>
</div>
