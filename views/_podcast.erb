<% @episodes.each do |episode| %>
  <form action="/stream-podcast" method="post">
    <input type="hidden" name="stream_podcast" value="<%= episode.enclosure_url %>">
    <input type="hidden" name="name" id="<%= episode.title %>">
    <input type="submit" value="<%= episode.title %>">
  </form>
<% end %>